<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/api/*</pattern>
	</excludes>
	
	<decorator name="default" page="index/default.jsp">
		<pattern>/*</pattern>
	</decorator>
    <decorator name="login" page="login/default.jsp">
        <pattern>/login*</pattern>
    </decorator>
</decorators>
